package mycaptain;

import java.io.*;
public class code1 
{
    public static void main()throws IOException
    {
    BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
int n=Integer.parseInt(br.readLine());
int y=n/(365*24*60);
int d=n%(365*24*60);
System.out.println(n+" minutes is approximately "+y+" years and "+d+" days");

}
}
